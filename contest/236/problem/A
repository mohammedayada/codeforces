#include <iostream>
#include <cmath>
#include <string>
#include <iomanip>
#include <algorithm>
using namespace std ;

int main()
{
	string s ;
	int arr[26] , c=0;
	cin>>s;
	for (int i=0 ; i<26 ; i++)
	{
		arr[i]=0;
	}
	for (int i=0 ; i<s.size() ; i++)
	{
		if (s[i]=='q')
			arr[0]++;
		else if (s[i]=='w')
			arr[1]++;
		else if (s[i]=='e')
			arr[2]++;
		else if (s[i]=='r')
			arr[3]++;
		else if (s[i]=='t')
			arr[4]++;
		else if (s[i]=='y')
			arr[5]++;
		else if (s[i]=='u')
			arr[6]++;
		else if (s[i]=='i')
			arr[7]++;
		else if (s[i]=='o')
			arr[8]++;
		else if (s[i]=='p')
			arr[9]++;
		else if (s[i]=='a')
			arr[10]++;
		else if (s[i]=='s')
			arr[11]++;
		else if (s[i]=='d')
			arr[12]++;
		else if (s[i]=='f')
			arr[13]++;
		else if (s[i]=='g')
			arr[14]++;
		else if (s[i]=='h')
			arr[15]++;
		else if (s[i]=='j')
			arr[16]++;
		else if (s[i]=='k')
			arr[17]++;
		else if (s[i]=='l')
			arr[18]++;
		else if (s[i]=='z')
			arr[19]++;
		else if (s[i]=='x')
			arr[20]++;
		else if (s[i]=='c')
			arr[21]++;
		else if (s[i]=='v')
			arr[22]++;
		else if (s[i]=='b')
			arr[23]++;
		else if (s[i]=='n')
			arr[24]++;
		else if (s[i]=='m')
			arr[25]++;
	

	}
	for (int i=0 ; i<26 ; i++)
	{
		if (arr[i]>0)
			c++;
	}
	
	if (c%2==0)
		cout<<"CHAT WITH HER!"<<endl;
	else
		cout<<"IGNORE HIM!"<<endl;
	return 0 ;
}
