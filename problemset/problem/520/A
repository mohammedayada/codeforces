#include <iostream>
#include <cmath>
#include <string>
#include <iomanip>
#include <algorithm>
using namespace std ;

int main()
{
	int n , arr[26];
	string s;
	cin>>n;
	cin>>s;
	if (n<26)
	{
		cout<<"NO"<<endl;
	    return 0 ;
	}
	for (int i=0 ; i<26 ; i++)
		arr[i]=0;
	for (int i=0 ; i<n ; i++)
	{
		if (s[i]=='q'||s[i]=='Q')
			arr[0]++;
		else if (s[i]=='w'||s[i]=='W')
			arr[1]++;
		else if (s[i]=='e'||s[i]=='E')
			arr[2]++;
		else if (s[i]=='r'||s[i]=='R')
			arr[3]++;
		else if (s[i]=='t'||s[i]=='T')
			arr[4]++;
		else if (s[i]=='y'||s[i]=='Y')
			arr[5]++;
		else if (s[i]=='u'||s[i]=='U')
			arr[6]++;
		else if (s[i]=='i'||s[i]=='I')
			arr[7]++;
		else if (s[i]=='o'||s[i]=='O')
			arr[8]++;
		else if (s[i]=='p'||s[i]=='P')
			arr[9]++;
		else if (s[i]=='a'||s[i]=='A')
			arr[10]++;
		else if (s[i]=='s'||s[i]=='S')
			arr[11]++;
		else if (s[i]=='d'||s[i]=='D')
			arr[12]++;
		else if (s[i]=='f'||s[i]=='F')
			arr[13]++;
		else if (s[i]=='g'||s[i]=='G')
			arr[14]++;
		else if (s[i]=='h'||s[i]=='H')
			arr[15]++;
		else if (s[i]=='j'||s[i]=='J')
			arr[16]++;
		else if (s[i]=='k'||s[i]=='K')
			arr[17]++;
		else if (s[i]=='l'||s[i]=='L')
			arr[18]++;
		else if (s[i]=='z'||s[i]=='Z')
			arr[19]++;
		else if (s[i]=='x'||s[i]=='X')
			arr[20]++;
		else if (s[i]=='c'||s[i]=='C')
			arr[21]++;
		else if (s[i]=='v'||s[i]=='V')
			arr[22]++;
		else if (s[i]=='b'||s[i]=='B')
			arr[23]++;
		else if (s[i]=='n'||s[i]=='N')
			arr[24]++;
		else if (s[i]=='m'||s[i]=='M')
			arr[25]++;
	}
	for (int i=0 ; i<26 ; i++)
	{
		if (arr[i]==0)
		{
			cout<<"NO"<<endl;
	        return 0 ;
		}
	}
	cout<<"YES"<<endl;
	return 0 ;
}
